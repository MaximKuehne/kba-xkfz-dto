{
    "name": "thag/kba-xkfz-dto",
    "description": "This is my package kba-xkfz-dto",
    "keywords": [
        "THAG",
        "kba-xkfz-dto"
    ],
    "homepage": "https://github.com/thag/kba-xkfz-dto",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Kuehne",
            "email": "maxim.kuehne@efk.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/ray": "^1.28",
        "wsdltophp/packagegenerator": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "THAG\\XKfz\\DtoGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "THAG\\XKfz\\DtoGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
